@article{pichePhysicalExaminationSpine2021,
  title = {Physical {{Examination}} of the {{Spine Using Telemedicine}}: {{A Systematic Review}}},
  shorttitle = {Physical {{Examination}} of the {{Spine Using Telemedicine}}},
  author = {Piche, Joshua and Butt, Bilal B. and Ahmady, Arya and Patel, Rakesh and Aleem, Ilyas},
