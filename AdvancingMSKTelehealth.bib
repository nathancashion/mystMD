@inproceedings{wangUnderstandingUserEngagement2019,
  title = {Understanding {{User Engagement Mechanisms}} on a {{Live Streaming Platform}}},
  booktitle = {{{HCI}} in {{Business}}, {{Government}} and {{Organizations}}. {{Information Systems}} and {{Analytics}}},
  author = {Wang, Xinwei and Wu, Dezhi},
