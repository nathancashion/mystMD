  file = {/Users/nately/Zotero/storage/RE8DWXA2/Hirschtritt and Kelly - 2024 - Four Tips for Improving Video Telehealth Patient-Clinician Communication.pdf;/Users/nately/Zotero/storage/ZYJZRWKR/2822571.html}
}

