  keywords = {COVID-19,disability,low back pain,MSHIcapstone,neck pain,obsidian,read,telemedicine,telerehabilitation},
  file = {/Users/nately/Zotero/storage/UY2FMK4P/Shah et al. - 2024 - Efficacy of telerehabilitation for spine pain during the Coronavirus pandemic lockdown a retrospect.pdf}
}

