@article{hartvigsenWhatLowBack2018,
  title = {What Low Back Pain Is and Why We Need to Pay Attention},
  author = {Hartvigsen, Jan and Hancock, Mark J and Kongsted, Alice and Louw, Quinette A and Ferreira, Manuela L and Genevay, St{\'e}phane and Hoy, Damian and Karppinen, Jaro and Pransky, Glenn and Sieper, Joachim and Smeets, Rob J and Underwood, Martin},
